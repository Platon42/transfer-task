{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "1st beneficiary"
      ],
      "pattern": "^(.*)$"
    },
    "street_line": {
      "$id": "#/properties/street_line",
      "type": "string",
      "title": "The Street_line Schema",
      "default": "",
      "examples": [
        "st1"
      ],
      "pattern": "^(.*)$"
    },
    "city": {
      "$id": "#/properties/city",
      "type": "string",
      "title": "The City Schema",
      "default": "",
      "examples": [
        "Moscow"
      ],
      "pattern": "^(.*)$"
    },
    "country": {
      "$id": "#/properties/country",
      "type": "string",
      "title": "The Country Schema",
      "default": "",
      "examples": [
        "RU"
      ],
      "pattern": "^(.*)$"
    },
    "postcode": {
      "$id": "#/properties/postcode",
      "type": "string",
      "title": "The Postcode Schema",
      "default": "",
      "examples": [
        "1111000"
      ],
      "pattern": "^(.*)$"
    }
  }
}